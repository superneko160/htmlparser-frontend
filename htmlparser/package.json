    },
    "devDependencies": {
        "@biomejs/biome": "2.3.8",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react-swc": "^4.2.2",
        "autoprefixer": "^10.4.23",
        "postcss": "^8.5.6",
        "tailwindcss": "^4.1.18",
        "typescript": "^5.9.2",
        "vite": "^7.3.0",
        "vitest": "^3.2.4"
    }
