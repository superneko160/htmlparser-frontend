    },
    "devDependencies": {
        "@biomejs/biome": "2.3.3",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@vitejs/plugin-react-swc": "^4.2.0",
        "autoprefixer": "^10.4.21",
        "postcss": "^8.5.6",
        "tailwindcss": "^4.1.16",
        "typescript": "^5.9.2",
        "vite": "^7.1.12",
        "vitest": "^3.2.4"
    }
